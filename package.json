{
	"name": "karaudio-admin",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc -b && vite build",
		"preview": "vite preview",
		"stylelint": "stylelint \"src/**/*.{css,scss,sass}\"",
		"stylelint:fix": "stylelint \"src/**/*.{css,scss,sass}\" --fix",
		"lint": "eslint --fix",
		"format": "prettier --write \"./src/**/*.{js,ts,jsx,tsx,json,css,scss,sass,md,vue}\" --ignore-path ./.prettierignore",
		"check": "npm run stylelint:fix && npm run lint && npm run format"
	},
	"dependencies": {
		"pinia": "^2.2.2",
		"vue": "^3.5.6",
		"vue-router": "^4.4.5"
	},
	"devDependencies": {
		"@eslint/js": "^9.11.0",
		"@vitejs/plugin-vue": "^5.1.4",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.11.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-vue": "^9.28.0",
		"globals": "^15.9.0",
		"postcss": "^8.4.47",
		"prettier": "^3.3.3",
		"sass": "^1.79.3",
		"stylelint": "^16.9.0",
		"stylelint-config-standard": "^36.0.1",
		"stylelint-order": "^6.0.4",
		"tailwindcss": "^3.4.12",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.6.0",
		"vite": "^5.4.6",
		"vue-tsc": "^2.1.6"
	}
}
